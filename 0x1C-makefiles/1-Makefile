# set GCC compiler as variable
CC=gcc

# object and source files to compile to final program
FILES=main.o school.o
SRC=main.c school.c

# This is the flag to compile source files to object code
CFLAGS=-Wall -Werror -c

all: prog

prog: $(FILES)
	$(CC) $(FILES) -o school

school.o: school.c
	$(CC) $(CFLAGS) school.c -o $@

main.o: main.c
	$(CC) $(CFLAGS) main.c -o $@
